<Window x:Class="Hashgen.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hashgen"
        mc:Ignorable="d"
        Title="HashGen" Height="450" Width="375">
    <Grid>
        <Button x:Name="button1" Content="Start" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="50" Width="100" Click="openfileselect"/>
        <ListView x:Name="Lbox" Margin="115,0,0,0">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        <Button x:Name="clear" Content="Clear" HorizontalAlignment="Left" Margin="34,373,0,0" VerticalAlignment="Center" Height="51" Width="76"/>

    </Grid>
</Window>
